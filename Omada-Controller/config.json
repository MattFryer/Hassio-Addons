{
    "name": "Omada Controller",
    "version": "4.2-arm64",
    "slug": "omada_controller",
    "description": "TP-Link Omada Controller software for managing WAPs",
    "url": "https://hub.docker.com/r/mbentley/omada-controller",
    "image": "mbentley/omada-controller",
    "webui": "https://[HOST]:[PORT:8043]",
    "startup": "application",
    "arch": ["aarch64"],
    "ports": {
        "8088/tcp": 8088,
        "8043/tcp": 8043,
        "8843/tcp": 8843,
        "29810/tcp": 29810,
        "29810/udp": 29810,
        "29811/tcp": 29811,
        "29811/udp": 29811,
        "29812/tcp": 29812,
        "29812/udp": 29812,
        "29813/tcp": 29813,
        "29813/udp": 29813  
    },
    "environment": {
        "MANAGE_HTTP_PORT": "8088",
        "MANAGE_HTTPS_PORT": "8043",
        "PORTAL_HTTP_PORT": "8088",
        "PORTAL_HTTPS_PORT": "8843",
        "SHOW_SERVER_LOGS": "true",
        "SHOW_MONGODB_LOGS": "false",
        "SMALL_FILES": "false",
        "SSL_CERT_NAME": "tls.crt",
        "SSL_KEY_NAME": "tls.key",
        "TZ": "Etc/UTC"
    },
    "map": [],
    "services": [],
    "options": {
    },
    "schema": {
    }
}
